{
  "name": "miljn-on-ic-ts",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "clean": "yarn backend:stop;",
    "backend:up": "yarn clean; dfx start --background; dfx deploy",
    "backend:stop": "dfx stop; if [ -d '.dfx' ]; then rm -rf .dfx; fi; if [ -d 'dist' ]; then rm -rf dist; fi",
    "code:style": "prettier --ignore-path=.eslintignore --check .",
    "code:style:fix": "prettier --ignore-path=.eslintignore --loglevel=log --write --list-different .",
    "code:analyse": "eslint  --ext .js --ext .vue --ext .yaml --ext .yml --ext .ts --ignore-path .eslintignore .",
    "code:analyse:fix": "eslint --ext .js --ext .vue --ext .yaml --ext .yml --ext .ts --ignore-path .eslintignore --fix .",
    "code:test": "jest",
    "code:test:ci": "echo \"yarn jest --ci --testResultsProcessor='jest-junit'\"",
    "code:quality-suite": "yarn git:pre-push",
    "postinstall": "if [ -n \"${HUSKY}\" ] && [ \"${HUSKY}\" -eq 0 ]; then echo 'INFO: skip husky for ci/cd' ; else husky install; fi",
    "git:pre-push": ".husky/pre-push"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/types": "^7.18.4",
    "@faker-js/faker": "^7.2.0",
    "@pinia/testing": "^0.0.12",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "babel-core": "^7.0.0-bridge.0",
    "buffer": "^6.0.3",
    "caniuse-lite": "^1.0.30001349",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^7.0.4",
    "lodash": "^4.17.21",
    "postcss": "^8.4.14",
    "postcss-import": "^14.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vue-tsc": "^0.37.1"
  },
  "dependencies": {
    "@dfinity/agent": "^0.11.3",
    "@dfinity/auth-client": "^0.11.3",
    "@dfinity/authentication": "^0.11.3",
    "@dfinity/candid": "^0.11.3",
    "@dfinity/identity": "^0.11.3",
    "@dfinity/principal": "^0.11.3",
    "@headlessui/vue": "^1.6.4",
    "@heroicons/vue": "^1.0.6",
    "@tiptap/core": "^2.0.0-beta.176",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-heading": "^2.0.0-beta.26",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/vue-3": "^2.0.0-beta.91",
    "@vueuse/core": "^8.6.0",
    "pinia": "^2.0.14",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-router": "4",
    "vuedraggable": "^2.24.3"
  }
}
